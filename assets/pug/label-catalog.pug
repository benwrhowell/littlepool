        +card-info-overlay('The Physics House Band', 'Mercury Fountain', 'product/physics.jpg')
      
      .column.is-6-mobile.is-3-tablet  
        +card-info-overlay('Luo','In The Dark','product/luo.jpg')
      .column.is-6-mobile.is-3-tablet  
        +card-info-overlay('The Physics House Band', 'Calypso + Slugabed remix', 'product/calypso.jpg')
    .columns.is-multiline.is-mobile         
      .column.is-12
